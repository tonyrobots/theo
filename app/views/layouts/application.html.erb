<!DOCTYPE html>
<html>
<head>
  <title>Theo</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<div id="container">

	<nav class="round">
	  <%= render("shared/topnav") %>
	</nav>

	<div id="logo" class="round">
	  <%= link_to(image_tag("theo.png", :border =>0), root_path) %>
	</div>
	<div id="flash">
	  <p id="notice"><%= notice %></p>
	  <p id="alert"><%= alert %></p>
	</div>
	<div id="body">
		<%= yield %>
	</div>
</div>
</body>
</html>
